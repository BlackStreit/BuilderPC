                            <TableColumn fx:id="tcCPUfrequency" editable="false" prefWidth="75.0" text="Частота" />
                              <TableColumn fx:id="tcCPUPower" editable="false" prefWidth="75.0" text="Энергопотребление" />
                              <TableColumn fx:id="tcCPUsocket" editable="false" prefWidth="75.0" text="Сокет" />
                              <TableColumn fx:id="tcCPUarchetype" editable="false" prefWidth="75.0" text="Архитектура" />
                          </columns>
                           <columnResizePolicy>
                              <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                           </columnResizePolicy>
                        </TableView>
                        <Button fx:id="btnCPUadd" layoutX="22.0" layoutY="545.0" mnemonicParsing="false" onAction="#btnCPUaddClick" prefHeight="56.0" prefWidth="81.0" text="Добавить" />
                        <Button fx:id="btnCPDelete" layoutX="117.0" layoutY="545.0" mnemonicParsing="false" onAction="#btnCPUdeleteClick" prefHeight="55.0" prefWidth="84.0" text="Удалить" />
