<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="728.0" prefWidth="1151.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.builderpc.HelloController">
   <children>
      <TabPane layoutX="20.0" layoutY="28.0" prefHeight="665.0" prefWidth="855.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Процессоры">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TableView fx:id="tableCPU" editable="true" layoutX="22.0" layoutY="14.0" prefHeight="514.0" prefWidth="819.0">
                          <columns>
                            <TableColumn fx:id="tcCPUnumber" editable="false" prefWidth="75.0" text="Артикул" />
